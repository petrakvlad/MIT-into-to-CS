

# return binary from int
def get_binary(n, numdigs):
    result = ''
    while n > 0:
        result = str(n%2) + result
        n = n // 2
        print(f"result: {result}  n= {n}")
    
    for i in range(numdigs - len(result)):
        result = '0' + result
    return result


print(get_binary(201, 16))

